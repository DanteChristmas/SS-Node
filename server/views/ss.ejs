<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Sullys Straps</title>
  </head>
  <body>
    <div class="list-group">
      <% posts.forEach((post) => { %>
        <a href="/post/<%= post.id %>" class="list-group-item">
          <h4 class="list-group-item-heading"><%= post.title %></h4>
          <p class="list-group-item-text"><%= post.author %></p>
        </a>
      <% }) %>
    </div>
  </body>
</html>
